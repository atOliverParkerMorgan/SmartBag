
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape android:shape="rectangle">

            <solid android:color="?attr/bgColor" />

            <size
                android:height="66dp"
                android:width="66dp" />
            <stroke
                android:width="1dp"
                android:color="#CCCCCC" />
            <gradient
                android:angle="45"
                android:startColor="#90EE90"
                android:endColor="#228B22"/>

        </shape>
    </item>


</layer-list>